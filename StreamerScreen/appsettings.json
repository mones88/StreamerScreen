{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Settings": {
        "ZonesToMonitor": "SMSL DO300 RAAT,SMSL DO300 HQ",
        "BindInterface": "en0",
        "Fullscreen": false,
        "ScreenControlEnabled": false,
        "ScreenOffCommand": "--display :0.0 --output DSI-1 --off",
        "ScreenOnCommand": "--display :0.0 --output DSI-1 --mode \"800x480\" --rotate inverted",
        "ScreenOffDelaySeconds": 50,
        "ScreenLowBrightenessCommand": "echo 30 > /sys/class/backlight/10-0045/brightness ",
        "ScreenNormalBrightnessCommad": "echo 128 > /sys/class/backlight/10-0045/brightness ",
        "ScreenLowBrightnessDelaySeconds": 10
    }
}
{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ZonesToMonitor": "SMSL DO300 RAAT,SMSL DO300 HQ",
    "BindInterface": "en0",
    "Fullscreen": "false",
    "ScreenControlEnabled": false,
    "ScreenOffCommand": "xrandr --display :0.0 --output DSI-1 --off",
    "ScreenOnCommand": "xrandr --display :0.0 --output DSI-1 --mode \"800x480\" --rotate inverted",
    "ScreenOffDelaySeconds": 60
}